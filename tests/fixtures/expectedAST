                ['fee' => 100500],
                ['deep' => ['id' => ['number' => 45]]]
            ]
]
]

{
    common: {
      + follow: false
        setting1: Value 1
      - setting2: 200
      - setting3: true
      + setting3: {
            key: value
        }
      + setting4: blah blah
      + setting5: {
            key5: value5
        }
